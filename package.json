{
  "name": "themecms",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "next build && npm run build:scss",
    "build:scss": "node-sass styles/main.scss static/dist/main.css",
    "build-scss:watch": "node-sass -w styles/ -o static/dist/",
    "dev": "cross-env NODE_ENV=devolopment SESSION_SECRET=themecms nodemon --watch ./server/src ./server/src/server.js & npm run build-scss:watch",
    "start": "cross-env NODE_ENV=production SESSION_SECRET=themecms node ./server/src/server.js",
    "analyze": "cross-env ANALYZE=1 next build"
  },
  "dependencies": {
    "antd": "^3.2.2",
    "async": "^2.6.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.1",
    "cookie-parser": "^1.4.3",
    "dotenv": "^5.0.1",
    "express-validator": "^5.0.2",
    "immutability-helper": "^2.6.5",
    "jsonwebtoken": "^8.1.1",
    "morgan": "^1.9.0",
    "next": "^5.0.0",
    "next-redux-wrapper": "^1.3.5",
    "node-sass": "^4.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^8.0.0",
    "babel-loader": "^7.1.3",
    "babel-plugin-import": "^1.6.5",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-wrap-in-js": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "glob": "^7.1.2",
    "nodemon": "^1.15.1",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.1",
    "raw-loader": "^0.5.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "styled-jsx-postcss": "^0.2.0",
    "webpack-bundle-analyzer": "^2.11.0"
  }
